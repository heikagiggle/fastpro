{
  "name": "@app/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "cli": "node scripts/cli.js",
    "build": "nx build web"
  },
  "nx": {
    "targets": {
      "generate-api": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/types/src/types",
          "commands": [
            "openapi-typescript \"http://127.0.0.1:4002/api/docs-yaml\" -o ./api.schema.ts"
          ]
        }
      },
      "deploy": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "dev",
        "options": {
          "env": {
            "HOSTED": "true"
          },
          "commands": [
            "npx nx run cli init",
            "npx nx run-many -t build -p web api --parallel=2",
            "npx nx build-event api",
            "npx nx deploy infra"
          ],
          "parallel": false
        },
        "configurations": {
          "dev": {},
          "prod": {
            "env": {
              "PRODUCTION_MODE": "true",
              "HOSTED": "true"
            }
          }
        }
      },
      "deploy-db": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "npx nx run cli load-db-url",
            "npx nx prisma-deploy api",
            "npx nx seed api"
          ],
          "parallel": false
        }
      }
    }
  },
  "private": true,
